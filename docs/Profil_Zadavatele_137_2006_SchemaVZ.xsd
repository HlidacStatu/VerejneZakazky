<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
		   xmlns:dc="http://purl.org/dc/elements/1.1/" 
		   xmlns:dcterm="http://purl.org/dc/terms/" 
		   xmlns:xml="http://www.w3.org/XML/1998/namespace" 
		   xmlns="urn:cz:isvz:mmr:schemas:vz-z-profilu-zadavatele:v100" 
		   xmlns:cct="urn:cz:isvs:micr:schemas:CoreComponentTypes:v1" 
		   xmlns:bus="urn:cz:isvs:micr:schemas:BusinessTypes:v2" 
		   xmlns:spa="urn:cz:isvs:micr:schemas:SpaceTypes:v1" 
		   xmlns:cmn="urn:cz:isvs:micr:schemas:CommonTypes:v1"
		   xmlns:dep="urn:cz:isvs:micr:schemas:DataElementsTypes:v1"
		   targetNamespace="urn:cz:isvz:mmr:schemas:vz-z-profilu-zadavatele:v100" 
		   elementFormDefault="qualified" 
		   attributeFormDefault="unqualified"
		   version="1.0.1.03">
	<xs:annotation>
		<xs:documentation xml:lang="cs">Návrh XML schématu zprávy pro získání dat o veřejné zakázce z profilu zadavatele  verze 1.01 - rozšířená</xs:documentation>
		<xs:appinfo>
			<rdf:RDF>
				<rdf:Description rdf:about="urn:cz:isvz:mmr:schemas:vz-z-profilu-zadavatele:v100" xml:lang="cs">
					<dc:creator>Petr Korotwitschka(korotwitschka@asd-software.cz)</dc:creator>
					<dc:contributor>Martin Nečaský (necasky@opendata.cz</dc:contributor>
					<dc:contributor>Jiří Skuhrovec(jiri.skuhrovec@eeip.cz)</dc:contributor>
					<dc:description>
						Rozšířené XML schéma zprávy pro získání dat o veřejné zakázce z profilu zadavatele rozšířený ve verzi 1.01 o nepovinné položky, schéma je zpětně kompatibilní
						version="1.0.1.01" - Změna ve struktuře seznamu dokumentů a dodatků VZ
						version="1.0.1.02" - Doplnění struktury migrace do VZ
						version="1.0.1.03" - u elementu cena_celkem_dle_smlouvy_bez_DPH opraven popis
						version="1.0.1.04" - rozsiren seznam podporovanych druhu zadavacich rizeni - element DruhZadavacihoRizeniType
					</dc:description>
					<dc:identifier/>
					<dc:publisher>Ministerstvo pro místní rozvoj ČR</dc:publisher>
					<dc:rights></dc:rights>
					<dc:source/>
					<dc:type>XML Schema</dc:type>
					<dcterm:created>2014-03-18</dcterm:created>
					<dcterm:valid/>
					<dc:subject>VZ request</dc:subject>
					<dc:format>text/xml</dc:format>
				</rdf:Description>
			</rdf:RDF>
		</xs:appinfo>
	</xs:annotation>
	<xs:import namespace="urn:cz:isvs:micr:schemas:CommonTypes:v1" schemaLocation="http://www.sluzby-isvs.cz/isdp/xsd/isvs_common.xsd"/>
	<xs:import namespace="urn:cz:isvs:micr:schemas:CoreComponentTypes:v1" schemaLocation="http://www.sluzby-isvs.cz/isdp/xsd/CoreComponentTypes.xsd"/>
	<xs:import namespace="urn:cz:isvs:micr:schemas:BusinessTypes:v2" schemaLocation="http://www.sluzby-isvs.cz/isdp/xsd/isvs_business.xsd"/>
	<xs:import namespace="urn:cz:isvs:micr:schemas:SpaceTypes:v1" schemaLocation="http://www.sluzby-isvs.cz/isdp/xsd/isvs_space.xsd"/>
	<xs:import namespace="urn:cz:isvs:micr:schemas:DataElementsTypes:v1" schemaLocation="http://www.sluzby-isvs.cz/isdp/xsd/isvs_dp.xsd"/>
	<xs:element name="profil" type="ProfilStructure"/>
	<xs:complexType name="ProfilStructure">
		<xs:annotation>
			<xs:documentation>Kořenová struktura pro předání seznamu VZ z profilu zadavatele</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="profil_kod" type="ProfilKodType"/>
			<xs:element name="zadavatel" type="ZadavatelStructure"/>
			<xs:element name="zakazka" type="ZakazkaStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UchazecStructure">
			<xs:annotation>
			<xs:documentation>
				Struktura pro předání základních informacích o uchazeči VZ
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ico" type="IcoType"/>
			<xs:element name="nazev_uchazece" type="bus:SubjektObchodniJmenoType">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Název nebo obchodní firma uchazeče v případě právnické osoby/ jméno, příjmení uchazeče v případě fyzické osoby</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="zeme_sidla" type="spa:StatKodType"/>
				<xs:element name="misto_podnikani" type="spa:StatKodType"/>
				<xs:element name="bydliste" type="spa:StatKodType"/>
			</xs:choice>
			<xs:element name="cena_s_dph" type="cct:CenaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Nabídková cena uchazeče s DPH</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sdruzeni" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Uchazeč je sdružením dodavatelů (identifikován je pak vedoucí účastník)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vyrazen" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Uchazeč byl/nebyl vyřazen
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="duvod_vyrazeni" type="DuvodVyrazeniUchazeceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Důvod vyřazení uchazeče (u zjednodušeného podlimitního řízení může být využito k informování uchazeče o vyřazení podle §60 a §76 ZVZ)
						Pokud byl uchazeč vyřazen, tak se povinně uvádí se druh důvodu
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nabidka" type="NabidkaStructure" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Nabídka - uvádí se seznam čísel částí VZ společně s nabídkovou cenou s DPH</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ZakazkaStructure">
		<xs:sequence>
		  <xs:annotation>
		  	<xs:documentation>Seznam všech dílčích veřejných zakázek evidovaných na profilu zadavatele. Rozsah seznamu je dán parametry samotného dotazu ze strany IS VZ</xs:documentation>
		  </xs:annotation>
			<xs:element name="vz" type="VZStructure"/>
			<xs:element name="uchazec" type="UchazecStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="dodavatel" type="DodavatelStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="url_zakazky" type="cmn:URLType" minOccurs="0"/>
			<xs:element name="url_zakazky_xml" type="cmn:URLType" minOccurs="0"/>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProfilKodType">
		<xs:annotation>
			<xs:documentation>
				Identifikátor profilu zadavatele
				- jedná se o evidenční číslo profilu zadavatele ve Věstníku veřejných zakázek přidělené na základě Oznámení profilu zadavatele
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:IdentifikatorType">
				<xs:minLength value="1"/>
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="VZStructure">
		<xs:sequence>
    	<xs:annotation>
		  	<xs:documentation>
				Základní informace charakterizující konkrétní zakázku
			</xs:documentation>
		  </xs:annotation>
			<xs:element name="kod_vz_na_profilu" type="KodVzNaProfiluType">
				<xs:annotation>
					<xs:documentation xml:lang="cz">
						Jednoznačný identifikátor veřejné zakázky evidované na profilu zadavatele odpovídající struktuře PrrVnnnnnnnn, kde:
						„P“ specifikuje, že se jedná o veřejnou zakázku uveřejněnou na profilu zadavatele
						„rr“ specifikuje poslední dvojčíslí letopočtu
						„V“ specifikuje, že se jedná o veřejnou zakázku
						„nnnnnnnn“ udává osmimístné pořadové číslo veřejné zakázky přidělené zadavatelem v příslušném roce
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kod_vz_na_usvzis" type="KodVZNaUsvzisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Evidenční číslo veřejné zakázky ve Věstníku veřejných zakázek - uvádí se, je-li veřejná zakázka uveřejněna ve Věstníku veřejných zakázek</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nazev_vz" type="NazevVZType"/>
			<xs:element name="stav_vz" type="StavVZType"/>
			<xs:element name="druh_zadavaciho_rizeni" type="DruhZadavacihoRizeniType"/>
			<xs:element name="zpusob_zahajeni_vz" type="ZpusobZahajeniVzType" minOccurs="0"/>
			<xs:element name="hlavni_cpv" type="PredmetKodType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Klasifikace hlavního předmětu pomocí CPV</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vedlejsi_cpv" type="PredmetKodType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Klasifikace vedlejšího předmětu pomocí kódů CPV</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="popis" type="PoznamkaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Popis zakázky - jedná se o popis předmětu VZ volným textem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nuts_hlavniho_mista_plneni" type="KrajNutsKodType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Místo plnění VZ - jedná se o NUTS kód hlavního místa plnění VZ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="predpokladana_hodnota" type="cct:CenaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Předpokládaná hodnota v Kč (bez DPH)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zpusob_hodnoceni" type="ZpusobHodnoceniType" minOccurs="0"/>
			<xs:element name="predmet_vz" type="PredmetZakazkyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Předmět zakázky</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typ_vz" type="TypZakazkyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Číselník typu VZ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lhuta_pro_podani_nabidek" type="cct:DatumCasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lhůta pro podání nabídek</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="elektronicka_aukce" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Jedná se o elektronickou aukci (§17 ZVZ)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ramcova_smlouva" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Jedná se o rámcovou smlouvu (§11 ZVZ)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aplikovan_par_101_zv" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0"/>
			<xs:element name="centralizovane_zadavani" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0"/>
			<xs:element name="zavedeni_dns" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Jedná se o zavedení DNS (§93 ZVZ)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="posledni_zmena" type="cct:DatumCasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Poslední změna ve zveřejněných datech</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zadavano_jmenem_sdruzeni" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Zadáváno jménem sdružení zadavatelů (§2 ZVZ)
						Pokud je zadáváno jménem sdružení zadavatelů, tak se povinně uvádí identifikace vedoucího účastníka pomocí IČO
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vedouci_ucastnik_ico" type="IcoType" minOccurs="0"/>
			<xs:element name="zadavano_jmenem_jineho_zadavatele" type="dep:DatovyPrvekJednoduchyTypIndikatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Zadáváno jménem jiného zadavatele (§3 ZVZ)
						Pokud je zadáváno jménem jiného zadavatele, tak se povinně uvádí IČO a název nebo obchodní firma jiného zadavatele
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ico_zadavatele_jehoz_jmenem_je_zadano" type="IcoType" minOccurs="0"/>
			<xs:element name="nazev_zadavatele_jehoz_jmenem_je_zadano" type="bus:SubjektObchodniJmenoType" minOccurs="0"/>
			<xs:element name="dokument" type="DokumentVzStructure" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Seznam dokumentů VZ.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ocekavany_pocet_vitezu_rs" type="cct:PocetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Očekávaný počet vítězných uchazečů rámcové smlouvy podle §89 odst 1)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vyuzita_dotace" type="ZdrojFinancovaniVzStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="datum_cas_zverejneni" type="cct:DatumCasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Čas zveřejnění - uvádí se datum včetně hodiny a minuty zpřístupnění VZ na profilu</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datum_zahajeni_vz" type="cct:DatumCasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Datum zahájení VZ (datum odeslání výzvy dodavatelům, v případě uveřejnění VZ na VVZ odpovídá datumu uveřejnění formuláře F02, F05 nebo F04, která je výzvou k soutěži)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datum_proplaceni" type="cct:DatumCasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Datum proplacení - datum faktické úhrady poslední platby celé VZ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cast_zakazky" type="CastVzStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="migrace" type="MigraceStructure" minOccurs="0" maxOccurs="unbounded"/>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RozpadStructure">
		<xs:sequence>
		  <xs:annotation>
		  	<xs:documentation>
				Specifikuje cenový rozpad ceny díla v jednotlivých letech pro uvedeného dodavatele
			</xs:documentation>
		  </xs:annotation>
			<xs:element name="rok_smlouvy" type="cct:PocetType" minOccurs="0"/>
			<xs:element name="cena_za_rok_bez_dph" type="cct:CenaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Skutečně uhrazená cena bez DPH v jednotlivých letech plnění – uvádí veřejný zadavatel, pokud došlo k realizaci veřejné zakázky</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cena_za_rok_s_dph" type="cct:CenaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="cz">Skutečně uhrazená cena s DPH v jednotlivých letech plnění - uvádí veřejný zadavatel, pokud došlo k realizaci veřejné zakázky</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KodVzNaProfiluType">
		<xs:annotation>
			<xs:documentation>Identifikátor VZ na profilu zadavatele</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:IdentifikatorType">
				<xs:minLength value="6"/>
				<xs:maxLength value="12"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="KodVZNaUsvzisType">
		<xs:annotation>
			<xs:documentation>Evidenční číslo VZ v IS VZUS</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:IdentifikatorType">
				<xs:minLength value="1"/>
				<xs:maxLength value="8"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="NazevVZType">
		<xs:annotation>
			<xs:documentation>Název VZ</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:TextType">
				<xs:minLength value="1"/>
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="StavVZType">
		<xs:annotation>
			<xs:documentation>Stav VZ</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:KodType">
				<xs:enumeration value="VZ neukončena">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Veřejná zakázka neukončena</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="VZ byla zadána">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Veřejná byla zadána</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="VZ byla zrušena">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Veřejná zakázka byla zrušena</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="Ukončeno plnění smlouvy na základě VZ">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Ukončeno plnění smlouvy na základě veřejné zakázky</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="DruhZadavacihoRizeniType">
		<xs:annotation>
			<xs:documentation>Druh zadávacího řízení</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:restriction base="cct:KodType">
        <xs:enumeration value="Zavedení DNS v otevřeném řízení">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Zavedení DNS v otevřeném řízení</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
        <xs:enumeration value="Zadávání VZ v DNS">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Zadávání VZ v DNS</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
        <xs:enumeration value="Přímé zadání">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Přímé zadání</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
        <xs:enumeration value="Minitendr">
					<xs:annotation>
						<xs:documentation xml:lang="cz">Minitendr</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
